{
  "name": "starter-app",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "npx tsx ./bin/www",
    "test": "jest",
    "migrate": "npx prisma migrate dev --name init",
    "generate": "npx prisma generate --schema=./prisma/schema.prisma",
    "studio": "npx prisma studio",
    "seed": "npm run generate && prisma db seed",
    "build": "tsc",
    "clean": "rm -rf dist && find . -name '*.js' -not -path './node_modules/*' -delete"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "argon2": "^0.43.0",
    "axios": "^1.7.9",
    "compose-middleware": "^5.0.1",
    "content-disposition": "^0.5.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "googleapis": "^144.0.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "typescript": "^5.6.3",
    "youtube-captions-scraper": "^2.0.3",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.18",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
